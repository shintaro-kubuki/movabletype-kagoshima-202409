<!DOCTYPE html><$mt:Include module="全般設定" strip_linefeeds="1"$>
<html lang="<$mt:BlogLanguage$>" itemscope itemtype="http://schema.org/WebPage">
<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# website: http://ogp.me/ns/website#">
  <meta charset="<$mt:PublishCharset$>">
  <title><$mt:PageTitle encode_html="1"$> <mt:If tag="PageFolder">| <mt:PageFolder><$mt:FolderLabel encode_html="1"$></mt:PageFolder> </mt:If>| <$mt:BlogName encode_html="1"$></title>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <link rel="shortcut icon" href="<$mt:BlogURL encode_html="1"$>images/favicon.ico">
  <link rel="start" href="<$mt:BlogURL encode_html="1"$>">
  <link rel="alternate" type="application/atom+xml" title="Recent Entries" href="<$mt:Link template="feed_recent"$>">
  <$mt:CanonicalLink$>
  
  <!-- Open Graph Protocol -->
  <meta property="og:locale" content="<$mt:BlogLanguage setvar="blog_lang"$><mt:If name="blog_lang" eq="ja">ja_JP<mt:Else><$mt:Var name="blog_lang"$></mt:If>">
  <meta property="og:title" content="<$mt:PageTitle encode_html="1"$> <mt:If tag="PageFolder">| <mt:PageFolder><$mt:FolderLabel encode_html="1"$></mt:PageFolder> </mt:If>| <$mt:BlogName encode_html="1"$>">
  <meta property="og:type" content="article">
  <meta property="og:url" content="<$mt:PagePermalink encode_html="1"$>">
  <meta property="og:image" content="<mt:If tag="CustomFieldValue" identifier="main_image_page"><mt:CustomFieldAsset identifier="main_image_page"><$mt:AssetThumbnailURL width="1200"$></mt:CustomFieldAsset><mt:ElseIf name="default_ogp_image"><mt:Asset id="$default_ogp_image"><$mt:AssetURL$></mt:Asset><mt:Else><$mt:BlogURL encode_html="1"$>images/ogimage.jpg</mt:If>">
  <meta property="og:description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
  <meta property="og:site_name" content="<$mt:BlogName encode_html="1"$>">
  <meta name="twitter:card" content="summary_large_image">
  <meta name="twitter:site" content="@<$mt:Var name='x-twitterVia'$>">
  <meta name="twitter:title" content="<$mt:PageTitle encode_html="1"$> <mt:If tag="PageFolder">| <mt:PageFolder><$mt:FolderLabel encode_html="1"$></mt:PageFolder> </mt:If>| <$mt:BlogName encode_html="1"$>">
  <meta name="twitter:description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
  <meta name="twitter:image" content="<mt:If tag="CustomFieldValue" identifier="main_image_page"><mt:CustomFieldAsset identifier="main_image_page"><$mt:AssetThumbnailURL width="1200"$></mt:CustomFieldAsset><mt:ElseIf name="default_ogp_image"><mt:Asset id="$default_ogp_image"><$mt:AssetURL$></mt:Asset><mt:Else><$mt:BlogURL encode_html="1"$>images/ogimage.jpg</mt:If>">
  <!-- Microdata -->
  <meta itemprop="description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
  <link itemprop="url" href="<$mt:BlogURL encode_html="1"$>news.html">
  <link itemprop="image" href="<mt:If tag="CustomFieldValue" identifier="main_image_page"><mt:CustomFieldAsset identifier="main_image_page"><$mt:AssetThumbnailURL width="1200"$></mt:CustomFieldAsset><mt:ElseIf name="default_ogp_image"><mt:Asset id="$default_ogp_image"><$mt:AssetURL$></mt:Asset><mt:Else><$mt:BlogURL encode_html="1"$>images/ogimage.jpg</mt:If>">
  <$mt:Include module="ヘッダー内script"$>
</mt:If>
</head>
<body> 
<mt:SetVarBlock name="this_page">webpage</mt:SetVarBlock>
<mt:SetVarBlock name="pages"><$mt:CustomFieldValue identifier="pagetype"$></mt:SetVarBlock>  
<$mt:Include module="ヘッダーメニュー"$>
<div class="UnderlayerJumbotron UnderlayerJumbotron__webpageimg"<mt:If name="UnderlayerJumbotron__bgcolor"> data-mt-site-editor-UnderlayerJumbotron__bgcolor="1"</mt:If> style="<mt:If tag="CustomFieldValue" identifier="main_image_page"><mt:CustomFieldAsset identifier="main_image_page">background-image:url(<$mt:AssetThumbnailURL width="1200"$>);</mt:CustomFieldAsset><mt:ElseIf name="UnderlayerJumbotron__webpageimg">background-image:url(<mt:Asset id="$UnderlayerJumbotron__webpageimg"><$mt:AssetURL$></mt:Asset>);</mt:If>">
  <div class="container mt-container">
	<mt:If tag="FolderDescription"><hgroup class="pageHeader-wrap"></mt:If>
      <mt:If tag="PageFolder">
        <h2 class="h2 mt-h2 pageHeader__title">
          <mt:TopLevelFolder><$mt:FolderLabel encode_html="1"$>
        </h2>
        	<mt:If tag="FolderDescription">
        		<p class="pageHeader__detial">
          		<$MTFolderDescription$>
        		</p>
        	</mt:If>
          </mt:TopLevelFolder>
          <mt:Else>
          <h2 class="h2 mt-h2 pageHeader__title">
            <$mt:PageTitle encode_html="1"$>
          </h2>
      	<mt:If tag="PageExcerpt">
          <p class="pageHeader__detial">
            <$mt:PageExcerpt no_generate="1"$>
          </p>
      	</mt:If>
      </mt:If>
    <mt:If tag="FolderDescription"></hgroup></mt:If>
  </div><!-- /.container -->
</div><!-- /.jumbotron -->

<main class="pagesWrap">
<mt:If name="pages" eq="ページ一覧">
<$mt:Include module="ウェブページ一覧ナビ"$>
  <!-- ページ一覧 -->
    <div class="pageslistWrap container-fluid mt-container-fluid">
     <div class="row mt-row">
      <div class="col-sm-12">
    	<article class="wysiwyg">
      <$mt:PageBody$>
      <$mt:PageMore$>
    	</article>
      </div>
    </div>
  </div>
    <$mt:Include module="ウェブページ一覧"$> 
</mt:If> 
<mt:If name="pages" eq="個別ページ">
	<mt:If tag="PageFolder">
		<$mt:Include module="ウェブページ一覧ナビ"$>
	</mt:If> 
  <!-- 個別ページ -->
  <div class="container-fluid mt-container-fluid">
    <div class="row mt-row">    
      <div class="col-sm-12">
        <article class="wysiwyg">
        	<mt:If tag="PageFolder">
        		<h1><$mt:PageTitle encode_html="1"$></h1>
        	</mt:If>
        <$mt:PageBody$>
        <$mt:PageMore$>
        </article>
      </div>
    </div>
  </div>
</mt:If>  
</main>

<$mt:Include module="ページ上部に戻るボタン"$>
<$mt:Include module="パンくずリスト"$>
<$mt:Include module="フッター"$>

<$mt:Include module="フッター内script"$>
</body>
</html>
